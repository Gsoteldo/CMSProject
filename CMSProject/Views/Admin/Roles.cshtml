@model CMSProject.Models.CmsViewModel

@{
    ViewData["Title"] = "Roles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Roles</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Role!.Role)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Roles!)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Role)
                </td>
            </tr>
        }
    </tbody>
</table>